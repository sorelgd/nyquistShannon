#API Documentation 
server_IP/api?ui=re_doc#operation/postUserCollection

#API acces
server_IP/api

# utiliser POSTMAN pour les tests
#Creation d'un nouvel utilisateur
POST
server_IP/api/users
{
  "firstname": "string",
  "lastname": "string",
  "creationdate": "2019-12-23T15:45:38Z",
  "updatedate": "2019-12-23T15:45:38Z"
}

#Liste des donnée d'un utilisateur
GET (id)
server_IP/api/users/{id}

#Liste des differents utilisateurs
GET (all)
/api/users/

#Suppression d'un utilisateur
DELETE
server_IP/api/users/{id}

#Modification des donnée d'un utilisateur
PUT
server_IP/api/users/{id}

{
  "firstname": "string",
  "lastname": "string",
  "updatedate": "2019-12-23T15:45:38Z"
}